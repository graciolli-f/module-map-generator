{
  "name": "module-map-generator",
  "version": "0.1.0",
  "description": "Generate module dependency maps from JavaScript codebases",
  "main": "src/index.js",
    "scripts": {
      "scan": "node src/walker.js",
      "analyze": "node src/analyze.js",
      "scan:commander": "node src/walker.js test-repos/commander.js module-map.json",
      "analyze:commander": "node src/analyze.js module-map.json dependency-analysis.json"
    },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0"
  }
}